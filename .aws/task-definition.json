{
    "family": "fargate-task-1",
    "executionRoleArn": "arn:aws:iam::938106397263:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "demo-container",
            "image": "938106397263.dkr.ecr.us-east-1.amazonaws.com/cicddemo:latest",
            "cpu": 512,
            "portMappings": [
              {
                "hostPort": 8080,
                "protocol": "tcp",
                "containerPort": 8080
              }
            ],
            "memory": 1024,
            "essential": true,
            "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-group": "/ecs/demo1",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "ecs"
              }
            }
        }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048"
  }
