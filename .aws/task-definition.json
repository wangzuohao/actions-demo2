{
    "family": "fargate-task-1",
    "executionRoleArn": "arn:aws-cn:iam::aws:policy/aws-service-role/AmazonECSServiceRolePolicy",
    "containerDefinitions": [
        {
            "name": "demo-container",
            "image": "062010366371.dkr.ecr.cn-northwest-1.amazonaws.com.cn/cicddemo:latest",
            "cpu": 512,
            "portMappings": [
              {
                "hostPort": 8080,
                "protocol": "tcp",
                "containerPort": 8080
              }
            ],
            "memory": 1024,
            "essential": true,
            "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                "awslogs-group": "/ecs/demo1",
                "awslogs-region": "cn-northwest-1",
                "awslogs-stream-prefix": "ecs"
              }
            }
        }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "1024",
    "memory": "2048"
  }
